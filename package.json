{
  "name": "drizzle-cube-express-example",
  "version": "1.0.0",
  "description": "Simple Express example with drizzle-cube",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsx watch server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "start": "tsx server.ts",
    "setup": "npm run install:server && npm run docker:up && sleep 5 && npm run db:migrate && npm run db:seed && npm run install:client",
    "install:client": "cd client && npm install",
    "install:server": "npm install --legacy-peer-deps",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:generate": "drizzle-kit generate",
    "typecheck": "tsc --noEmit",
    "ci:all": "npm ci && cd client && npm ci"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "cors": "^2.8.5",
    "drizzle-cube": "^0.2.0",
    "drizzle-orm": "^0.45.0",
    "express": "^5.0.0",
    "postgres": "^3.4.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "rollup": "^4.24.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "analytics",
    "drizzle",
    "express",
    "cube",
    "semantic-layer"
  ],
  "author": "Drizzle Cube",
  "license": "MIT"
}
